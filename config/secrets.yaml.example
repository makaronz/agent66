# Secrets configuration template
# Copy this file to secrets.yaml and add your actual API keys and secrets
# IMPORTANT: This file should NEVER be committed to version control

# Exchange API keys and secrets
exchanges:
  binance:
    api_key: "your_binance_api_key_here"
    api_secret: "your_binance_api_secret_here"

  bybit:
    api_key: "your_bybit_api_key_here"
    api_secret: "your_bybit_api_secret_here"

  oanda:
    api_key: "your_oanda_api_key_here"
    api_secret: "your_oanda_api_secret_here"

# Database credentials
database:
  password: "your_database_password_here"

# Redis credentials
redis:
  password: "your_redis_password_here"

# External service API keys
external_apis:
  news_api_key: "your_news_api_key_here"
  sentiment_api_key: "your_sentiment_api_key_here"
  weather_api_key: "your_weather_api_key_here"  # If using weather data for analysis

# Notification service credentials
notifications:
  slack_webhook_url: "your_slack_webhook_url_here"
  telegram_bot_token: "your_telegram_bot_token_here"
  email_smtp_password: "your_smtp_password_here"

# Authentication and security
security:
  jwt_secret: "your_jwt_secret_key_here"
  encryption_key: "your_encryption_key_here"

# Monitoring service credentials
monitoring:
  datadog_api_key: "your_datadog_api_key_here"
  prometheus_remote_write_key: "your_prometheus_key_here"