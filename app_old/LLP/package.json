{
  "name": "smc_trading_agent",
  "private": true,
  "packageManager": "pnpm@8.15.4",
  "engines": {
    "node": ">=18"
  },
  "version": "1.0.0-beta.1",
  "type": "module",
  "scripts": {
    "client:dev": "vite",
    "build": "tsc -b && vite build",
    "build:vite-only": "vite build",
    "build:analyze": "tsc -b && vite build --mode analyze",
    "lint": "eslint .",
    "preview": "vite preview",
    "check": "tsc --noEmit",
    "server:dev": "nodemon",
    "dev": "concurrently \"npm run client:dev\" \"npm run server:dev\"",
    "docs:generate": "./scripts/generate-api-docs.sh",
    "cem:analyze": "custom-elements-manifest analyze",
    "cem:watch": "custom-elements-manifest analyze --watch",
    "docs:validate": "swagger-cli validate docs/openapi-express.json && swagger-cli validate docs/openapi-fastapi.json",
    "docs:serve": "http-server docs/api -p 8080 -o",
    "docs:build": "npm run docs:generate && npm run docs:validate",
    "api:start": "concurrently \"npm run server:dev\" \"python -m uvicorn api_fastapi:app --host 0.0.0.0 --port 8000\"",
    "api:test": "newman run docs/postman/smc-trading-agent-collection.json -e docs/postman/development.json",
    "sbom:generate": "python tools/generate_sbom.py --output docs/sbom --format json",
    "security:scan": "python tools/scan_vulnerabilities.py --mode full",
    "security:quick": "./tools/sbom_workflow.sh --scan quick --verbose",
    "security:check": "python tools/scan_vulnerabilities.py --mode quick",
    "security:report": "python tools/scan_vulnerabilities.py --mode report",
    "deploy:vercel": "vercel --prod",
    "deploy:preview": "vercel",
    "deploy:build": "pnpm run build && pnpm run deploy:vercel"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.1",
    "@simplewebauthn/browser": "^10.0.0",
    "@simplewebauthn/server": "^10.0.0",
    "@supabase/supabase-js": "^2.54.0",
    "@tanstack/react-query": "^5.84.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "ccxt": "^4.4.99",
    "clsx": "^2.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.4.1",
    "framer-motion": "^12.23.12",
    "helmet": "^8.0.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.511.0",
    "qrcode": "^1.5.4",
    "rate-limit-redis": "^4.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.62.0",
    "react-hot-toast": "^2.5.2",
    "react-router-dom": "^7.3.0",
    "recharts": "^3.1.2",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.7",
    "speakeasy": "^2.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tailwind-merge": "^3.0.2",
    "twilio": "^5.3.5",
    "web-vitals": "^5.1.0",
    "zod": "^4.0.17",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@cyclonedx/cdxgen": "^10.8.1",
    "@eslint/js": "^9.25.0",
    "@openapitools/openapi-generator-cli": "^2.22.0",
    "@redocly/cli": "^2.0.5",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.19",
    "@types/crypto-js": "^4.2.2",
    "@types/date-fns": "^2.6.3",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.15.30",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/socket.io-client": "^3.0.0",
    "@types/speakeasy": "^2.0.10",
    "@vercel/node": "^5.3.6",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "babel-plugin-react-dev-locator": "^1.0.0",
    "concurrently": "^9.2.0",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "jsdom": "^26.1.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.3",
    "postman-to-openapi": "^3.0.1",
    "redoc-cli": "^0.13.21",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-plugin-trae-solo-badge": "^1.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "customElements": "custom-elements.json"
}
