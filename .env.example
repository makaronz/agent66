# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Backend Supabase Configuration (Server-side only)
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here
SUPABASE_URL=your_supabase_project_url_here

# Encryption Configuration
ENCRYPTION_KEY=your-32-character-encryption-key-here-change-this-in-production
BINANCE_API_KEY=your_binance_api_key_here
BINANCE_API_SECRET=your_binance_api_secret_here

# Optional: Database Configuration (if using external database)
# DATABASE_URL=postgresql://user:password@localhost:5432/smc_trading

# Optional: Redis Configuration (if using Redis for caching)
# REDIS_URL=redis://localhost:6379/0

# Optional: Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=logs/smc_trading.log

# Optional: Monitoring Configuration
# PROMETHEUS_PORT=9090
# GRAFANA_PORT=3000

# Development Configuration
NODE_ENV=development
PORT=3002

# External Services Configuration (Development)
VAULT_ENABLED=false
REDIS_ENABLED=false
VAULT_ADDR=http://127.0.0.1:8200
VAULT_TOKEN=root

# Development Flags (Hotfix Iteration 1)
VITE_ENABLE_STRICT_MODE=false
VITE_ENABLE_DEBUG_LOGS=true
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_WEBSOCKET_RECONNECT_DELAY=1000
VITE_MAX_RECONNECT_ATTEMPTS=5

# Trading Configuration
DEFAULT_RISK_PERCENTAGE=2
MAX_CONCURRENT_TRADES=5
DEFAULT_STOP_LOSS_PERCENTAGE=1
DEFAULT_TAKE_PROFIT_PERCENTAGE=3

# API Rate Limiting (Enhanced for Hotfix Iteration 1)
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
VITE_BINANCE_RATE_LIMIT_REQUESTS=1200
VITE_BINANCE_RATE_LIMIT_INTERVAL=60000
VITE_MARKET_DATA_THROTTLE_MS=60000
VITE_WEBSOCKET_PING_INTERVAL=30000
VITE_WEBSOCKET_PONG_TIMEOUT=10000