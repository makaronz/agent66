{
  "info": {
    "name": "SMC Trading Agent API",
    "description": "Comprehensive API collection for SMC Trading Agent with authentication, trading, and market data endpoints",
    "version": "1.0.0",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "auth": {
    "type": "bearer",
    "bearer": [
      {
        "key": "token",
        "value": "{{jwt_token}}",
        "type": "string"
      }
    ]
  },
  "event": [
    {
      "listen": "prerequest",
      "script": {
        "type": "text/javascript",
        "exec": [
          "// Set base URL based on environment",
          "if (pm.environment.get('environment') === 'production') {",
          "    pm.globals.set('base_url', 'https://api.smctradingagent.com');",
          "} else if (pm.environment.get('environment') === 'staging') {",
          "    pm.globals.set('base_url', 'https://staging-api.smctradingagent.com');",
          "} else {",
          "    pm.globals.set('base_url', 'http://localhost:3000');",
          "}",
          "",
          "// Set API version",
          "pm.globals.set('api_version', 'v1');"
        ]
      }
    }
  ],
  "variable": [
    {
      "key": "base_url",
      "value": "http://localhost:3000",
      "type": "string"
    },
    {
      "key": "api_version",
      "value": "v1",
      "type": "string"
    },
    {
      "key": "jwt_token",
      "value": "",
      "type": "string"
    }
  ],
