{
  "name": "film_time_tracker",
  "private": true,
  "packageManager": "pnpm@8.15.4",
  "engines": {
    "node": ">=18"
  },
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "client:dev": "vite",
    "build": "tsc -b && vite build",
    "build:vite-only": "vite build",
    "build:analyze": "tsc -b && vite build --mode analyze",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "check": "tsc --noEmit",
    "server:dev": "tsx watch backend/src/server.ts",
    "dev": "concurrently \"pnpm run client:dev\" \"pnpm run server:dev\"",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx backend/src/seed.ts",
    "agent:start": "python main.py",
    "agent:resilient": "python -c \"import subprocess, time, sys; [subprocess.run([sys.executable, 'main.py']) or time.sleep(5) for _ in iter(int, 1)]\"",
    "agent:health": "curl -f http://localhost:8001/health || exit 1",
    "agent:monitor": "watch -n 5 'curl -s http://localhost:8001/health/detailed | python -m json.tool'",
    "system:health": "pnpm run agent:health && curl -f http://localhost:5000/api/health",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:backend": "vitest run --config backend/vitest.config.ts",
    "test:frontend": "vitest run --config frontend/vitest.config.ts",
    "test:integration": "vitest run --config test/integration/vitest.config.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "security:generate-secrets": "tsx scripts/generate-secrets.ts",
    "security:validate-config": "tsx scripts/validate-config.ts",
    "security:check-secrets": "tsx scripts/check-exposed-secrets.ts",
    "security:audit": "pnpm audit && pnpm audit",
    "security:scan": "python tools/scan_vulnerabilities.py --mode full",
    "security:quick": "pnpm run security:audit && pnpm run security:check-secrets",
    "docs:generate": "./scripts/generate-api-docs.sh",
    "cem:analyze": "custom-elements-manifest analyze",
    "cem:watch": "custom-elements-manifest analyze --watch",
    "docs:validate": "swagger-cli validate docs/openapi-express.json && swagger-cli validate docs/openapi-fastapi.json",
    "docs:serve": "http-server docs/api -p 8080 -o",
    "docs:build": "pnpm run docs:generate && pnpm run docs:validate",
    "api:start": "concurrently \"pnpm run server:dev\" \"python -m uvicorn api_fastapi:app --host 0.0.0.0 --port 8000\"",
    "api:test": "newman run docs/postman/smc-trading-agent-collection.json -e docs/postman/development.json",
    "sbom:generate": "python tools/generate_sbom.py --output docs/sbom --format json",
    "security:report": "python tools/scan_vulnerabilities.py --mode report",
    "setup:dev": "pnpm run security:generate-secrets && cp .env.development .env.local",
    "setup:prod": "pnpm run security:generate-secrets && cp .env.template .env.production",
    "deploy:vercel": "vercel --prod",
    "deploy:preview": "vercel",
    "deploy:build": "pnpm run build && pnpm run deploy:vercel",
    "docker:build": "docker build -t smc-trading-agent .",
    "docker:run": "docker run -p 5000:5000 -p 8001:8001 smc-trading-agent",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "monitoring:start": "docker-compose -f docker-compose.monitoring.yml up -d",
    "monitoring:stop": "docker-compose -f docker-compose.monitoring.yml down"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.4",
    "@babel/traverse": "^7.28.5",
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.1",
    "@prisma/client": "^5.22.0",
    "@simplewebauthn/browser": "^10.0.0",
    "@simplewebauthn/server": "^10.0.0",
    "@supabase/supabase-js": "^2.54.0",
    "@tanstack/react-query": "^5.84.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.4.1",
    "framer-motion": "^12.23.12",
    "helmet": "^8.0.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.511.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.10",
    "pdf-lib": "^1.17.1",
    "prisma": "^5.22.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.62.0",
    "react-hot-toast": "^2.5.2",
    "react-router-dom": "^7.3.0",
    "recharts": "^3.1.2",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tailwind-merge": "^3.0.2",
    "web-vitals": "^5.1.0",
    "ws": "^8.18.3",
    "zod": "^4.0.17",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@cyclonedx/cdxgen": "^11.11.0",
    "@eslint/js": "^9.25.0",
    "@openapitools/openapi-generator-cli": "^2.22.0",
    "@redocly/cli": "^2.0.5",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.19",
    "@types/date-fns": "^2.6.3",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.11",
    "@types/node": "^22.15.30",
    "@types/nodemailer": "^6.4.15",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/socket.io-client": "^3.0.0",
    "@vercel/node": "^4.0.0",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "babel-plugin-react-dev-locator": "^1.0.0",
    "concurrently": "^9.2.0",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "jsdom": "^26.1.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.3",
    "postman-to-openapi": "^3.0.1",
    "redoc-cli": "^0.8.5",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-plugin-trae-solo-badge": "^1.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "customElements": "custom-elements.json"
}
